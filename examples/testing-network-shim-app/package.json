{
    "name": "testing-network-shim-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "axios": "^0.21.0",
        "json-server": "^0.16.3",
        "json-server-reset": "^1.3.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.1",
        "web-vitals": "^0.2.4"
    },
    "devDependencies": {
        "@dhis2/cli-utils-cypress": "file:../../packages/cli",
        "@dhis2/cypress-commands": "file:../../packages/cypress-commands"
    },
    "resolutions": {
        "@dhis2/cli-utils-cypress": "file:../../packages/cli",
        "@dhis2/cypress-commands": "file:../../packages/cypress-commands"
    },
    "scripts": {
        "serve": "json-server src/json-server/db.js --routes ./src/json-server/routes.json --middlewares ./node_modules/json-server-reset --watch -p 1337",
        "cypress:open": "d2-utils-cypress open --appStart 'yarn start'",
        "cypress:run": "d2-utils-cypress run --appStart 'yarn start'",
        "cypress:capture": "d2-utils-cypress run --capture --appStart 'yarn start' --serverMinorVersion 36",
        "cypress:stub": "d2-utils-cypress run --stub --appStart 'yarn start' --serverMinorVersion 36",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
